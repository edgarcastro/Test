{
  "name": "mobile-api",
  "version": "1.0.0",
  "description": "",
  "main": "./src/index.js",
  "scripts": {
    "test": "jest src",
    "test:pact": "jest pacts --detectOpenHandles",
    "start": "nodemon src/index.js",
    "start:debug": "nodemon --inspect src/index.js",
    "lint": "eslint src",
    "redoc-bundle": "redoc-cli bundle ./spec/api-docs.yml --output ../docs/mobile-api/index.html",
    "redoc-reload": "cd .. && live-server --watch=docs/ --open=docs/",
    "redoc-watch": "watch \"npm run redoc-bundle\" ./spec",
    "api-doc": "concurrently \"npm run redoc-reload\" \"npm run redoc-watch\"",
    "test:spec": "dredd"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@pact-foundation/pact-node": "8.1.0",
    "chai": "4.1.2",
    "concurrently": "4.1.0",
    "dredd": "8.0.6",
    "eslint": "5.15.3",
    "husky": "1.3.1",
    "jest": "24.5.0",
    "live-server": "1.2.1",
    "moxios": "0.4.0",
    "nodemon": "1.18.9",
    "sinon": "7.2.2",
    "supertest": "3.3.0",
    "watch": "1.0.2"
  },
  "dependencies": {
    "@condor-labs/metrics": "1.2.1",
    "axios": "0.18.0",
    "body-parser": "1.18.3",
    "dotenv": "6.0.0",
    "express": "4.16.3",
    "express-joi-validation": "0.2.1",
    "file-type": "10.7.0",
    "form-data": "2.3.3",
    "helmet": "3.13.0",
    "joi": "14.3.1",
    "jsonwebtoken": "8.3.0",
    "moment": "2.22.2",
    "multer": "1.4.1",
    "newrelic": "5.6.1",
    "query-string": "6.2.0",
    "redoc-cli": "0.8.3",
    "stream-to-promise": "2.2.0",
    "winston": "3.2.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm test",
      "pre-push": "npm run lint && npm test"
    }
  }
}
